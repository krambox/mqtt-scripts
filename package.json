{
  "name": "mqtt-scripts",
  "version": "1.1.0",
  "description": "Node.js based script runner for use in MQTT based Smart Home environments",
  "main": "index.js",
  "scripts": {
    "test": "camo-purge ; xo && nyc mocha test.js --exit && nyc report --reporter=text-lcov | coveralls --force",
    "testonly": "mocha test.js --exit",
    "lintfix": "xo --fix"
  },
  "author": "Sebastian 'hobbyquaker' Raff <hobbyquaker@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "coffee-compiler": "^0.3.2",
    "coffee-script": "^1.12.7",
    "mqtt": "^2.18.8",
    "node-schedule": "^1.3.0",
    "suncalc": "^1.8.0",
    "watch": "^1.0.2",
    "yalm": "^4.1.0",
    "yargs": "^12.0.2"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "engineStrict": true,
  "preferGlobal": true,
  "devDependencies": {
    "camo-purge": "latest",
    "coveralls": "latest",
    "gulp": "latest",
    "jsdoc-to-markdown": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "should": "latest",
    "stream-splitter": "latest",
    "timekeeper": "^2.1.2",
    "xo": "latest"
  },
  "directories": {
    "doc": "doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hobbyquaker/mqtt-scripts"
  },
  "keywords": [
    "MQTT",
    "javascript",
    "coffeescript",
    "node.js",
    "npm",
    "sandbox",
    "vm",
    "Smart",
    "Home",
    "Internet",
    "of",
    "Things",
    "IoT"
  ],
  "bugs": {
    "url": "https://github.com/hobbyquaker/mqtt-scripts/issues"
  },
  "homepage": "https://github.com/hobbyquaker/mqtt-scripts",
  "xo": {
    "space": 4,
    "ignores": [
      "testscripts/**",
      "test.js",
      "Gulpfile.js",
      "mockdate.js"
    ]
  },
  "nyc": {
    "include": [
      "index.js",
      "config.js",
      "sandbox/*.js"
    ]
  }
}
